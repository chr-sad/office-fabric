{
  "name": "@fluentui-vue/components",
  "version": "2.0.0-rc.17",
  "main": "./dist/components.umd.js",
  "module": "./lib/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "src",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist; rimraf lib; rimraf types",
    "build": "run-p build-only build:module",
    "build:module": "tsc -p tsconfig.module.json; tsc-alias -p tsconfig.module.json",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "dependencies": {
    "@fluentui-vue/file-type-icons": "workspace:^2.0.0",
    "@fluentui-vue/hooks": "workspace:^1.0.0",
    "@fluentui-vue/icons": "workspace:^1.2.0",
    "@fluentui-vue/style-utilities": "workspace:^1.1.0",
    "@fluentui-vue/theme": "workspace:^1.1.0",
    "@fluentui-vue/utilities": "workspace:^1.0.2",
    "@fluentui/dom-utilities": "^2.2.9",
    "@fluentui/merge-styles": "^8.5.10",
    "tslib": "^2.5.2"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.14",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "esbuild": "^0.17.19",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1",
    "vue-tsc": "^1.6.5"
  }
}
